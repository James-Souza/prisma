// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Characters {
  id             String   @unique
  name           String
  level          String
  characterPower String
  email          String
  darksteel      String
  createdAt      DateTime
  updatedAt      DateTime
  hp             String
  mp             String
  physicalATK    String
  physicalDEF    String
  speelDEF       String
}

model Wallets {
  email     String   @unique
  draco     String
  hydra     String
  createdAt DateTime
  updatedAt DateTime
}

model Areas {
  id   String @unique
  name String
}

model Session {
  id           String   @unique
  name         String
  area         String
  initialValue Float
  finalValue   Float
  status       String
  createdAt    DateTime
  updatedAt    DateTime
  serverDate   DateTime
}
